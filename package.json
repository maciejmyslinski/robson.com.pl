{
  "name": "robson.com.pl",
  "description": "robson.com.pl",
  "version": "1.0.0",
  "author": "Maciej Myśliński <hello@maciejmyslinski.com>",
  "dependencies": {
    "gatsby": "^1.8.12",
    "gatsby-link": "^1.6.8",
    "gatsby-plugin-react-helmet": "^1.0.3",
    "typography": "^0.16.6",
    "uuid": "^3.1.0"
  },
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o public",
    "prettier": "prettier-eslint 'src/**/*.js' --write --list-different"
  },
  "babel": {
    "presets": ["env", "react"]
  },
  "devDependencies": {
    "@storybook/react": "^3.2.8",
    "babel-jest": "^21.0.2",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.6.0",
    "eslint": "^4.7.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.5.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "firebase-tools": "^3.9.2",
    "gatsby-plugin-styled-components": "^1.0.3",
    "jest": "^21.1.0",
    "lint-staged": "^4.2.1",
    "prettier": "^1.5.2",
    "prettier-eslint-cli": "^4.3.2",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1"
  },
  "jest": {
    "setupFiles": ["./test/jestsetup.js"],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": ["<rootDir>/node_modules/enzyme-to-json/serializer"]
  },
  "lint-staged": {
    "src/**/*.js": ["prettier-eslint --write --list-different", "git add"]
  }
}
